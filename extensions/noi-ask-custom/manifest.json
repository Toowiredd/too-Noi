{
  "manifest_version": 3,
  "name": "@noi/ask-custom",
  "version": "0.1.0",
  "homepage": "https://github.com/lencx/Noi/tree/main/extensions/noi-ask-custom",
  "description": "The best assistant for batch asking and quick typing of prompts.",
  "content_scripts": [
    {
      "matches": [
        "https://example.com/*"
      ],
      "js": ["main.js"],
      "run_at": "document_end",
      "world": "MAIN"
    }
  ]
  // TODO: Update the URL to match the custom AI Chat
  // The purpose of this TODO is to ensure that the URL specified in the manifest.json file matches the URL of the custom AI chat service being implemented. This is necessary for the extension to correctly identify and interact with the custom AI chat service.
}
